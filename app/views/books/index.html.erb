<%= render 'list', user: @user %>

<h1>Books</h1>

<table>
  <tr>
   <th>本のタイトル</th>
   <th>感想</th>
   <th></th>
  </tr>

  <% @books.each do |book| %>
  <tr>
   <td> <%= image_tag book.user.get_profile_image(100,100) %> </td>
   <td> <%=book.title %> </td>
   <td> <%=book.body %> </td>
  </tr>
  <% end %>
</table>